<input type="radio"
		 id={{inputId}}
					name={{name}}
		 autocomplete={{autoComplete}}
					autofocus={{autoFocus}}
		 disabled={{disabled}}
					form={{form}}
		 formnovalidate={{formNoValidate}}
					readonly={{readOnly}}
		 accesskey={{accessKey}}
					contenteditable={{contentEditable}}
		 contextMenu={{contextMenu}}
					draggable={{draggable}}
		 dir={{dir}}
					dropzone={{dropZone}}
		 hidden={{hidden}}
					lang={{lang}}
		 spellcheck={{spellCheck}}
					tabindex={{tabIndex}}
		 title={{title}}
					translate={{translate}}
		 data-parsley-required={{required}}
					data-parsley-required-message={{requiredMessage}}
		 required={{required}}
					data-test-automation-id={{testAutomationId}}
		 data-parsley-group={{validationGroup}}
					data-parsley-class-handler={{validationClassHandlerComputed}}
		 data-parsley-errors-container={{errorMessagesContainerComputed}}
					data-parsley-error-message={{errorMessage}}
		 checked={{isChecked}}> {{label}}
